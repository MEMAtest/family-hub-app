{
  "name": "family-hub-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:bugfixes": "tsx tests/bug-fixes/run-all-tests.ts",
    "test:bugfix:calendar": "tsx tests/bug-fixes/calendar-event-update.test.ts",
    "test:bugfix:budget": "tsx tests/bug-fixes/budget-modals-family-members.test.ts",
    "test:bugfix:chart": "tsx tests/bug-fixes/chart-visual-fix.test.ts",
    "test:smoke": "tsx tests/smoke/run-smoke-tests.ts",
    "test:persistence": "node --import tsx tests/persistence/run-persistence-tests.ts",
    "test:e2e": "env -u NO_COLOR playwright test",
    "test:e2e:confidence": "env -u NO_COLOR playwright test --workers=1 tests/e2e/bootstrap-resilience.spec.ts tests/e2e/budget.spec.ts tests/e2e/mobile-persistence.spec.ts tests/e2e/full-app-critical.spec.ts && npm run test:persistence",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "model:region": "tsx scripts/property-model/derive-region.ts",
    "model:ppd": "tsx scripts/property-model/download-ppd.ts",
    "model:ukhpi": "tsx scripts/property-model/download-ukhpi.ts",
    "model:onspd": "tsx scripts/property-model/download-onspd.ts",
    "model:planning": "tsx scripts/property-model/download-planning.ts",
    "model:dataset": "tsx scripts/property-model/build-dataset.ts",
    "model:train": "tsx scripts/property-model/train-model.ts",
    "model:ingest": "npm run model:ppd && npm run model:ukhpi && npm run model:onspd && npm run model:planning",
    "model:build": "npm run model:ingest && npm run model:dataset && npm run model:train",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
