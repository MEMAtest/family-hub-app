    "express": "^5.1.0",
    "garmin-connect": "^1.6.2",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "helmet": "^8.1.0",
    "ical-generator": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "moment": "^2.30.1",
