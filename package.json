    "next": "14.2.35",
    "node-cron": "^4.2.1",
    "node-ical": "^0.21.0",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.16.2",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
